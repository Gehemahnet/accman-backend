generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  uuid        String    @id @default(uuid())
  userName    String    @unique
  name        String?
  surname     String?
  lastName    String?
  email       String    @unique
  dateOfBirth DateTime?
  role        Role      @default(USER)
}

enum Role {
  USER
  ADMIN
}
